{
  "name": "@appik-studio/expo-pressreader",
  "version": "0.0.21",
  "description": "Expo plugin for PressReader SDK - provides access to newspapers and magazines",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "android",
    "ios",
    "plugin/build",
    "app.plugin.js",
    "expo-module.config.json"
  ],
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "bump-version": "bun run scripts/bump-version.ts",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "build:plugin": "tsc --build ./plugin/tsconfig.json",
    "ios:refresh": "bun run build:plugin && cd example && bun run expo:prebuild-clean-ios && bun run ios:pod-install && bun run ios",
    "android:refresh": "bun run build:plugin && cd example && bun run expo:prebuild-clean-android && bun run android"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-pressreader",
    "ExpoPressReader"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Appik-Studio/expo-pressreader.git"
  },
  "bugs": {
    "url": "https://github.com/Appik-Studio/expo-pressreader/issues"
  },
  "author": "Livio Gamassia <livio@appik-studio.ch> (Appik-Studio)",
  "license": "MIT",
  "homepage": "https://github.com/Appik-Studio/expo-pressreader#readme",
  "devDependencies": {
    "@types/react": "~19.0.0",
    "expo": "~52.0.0",
    "expo-module-scripts": "^4.1.6",
    "husky": "^9.1.7",
    "react-native": "*",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "expo": {
    "plugin": "./app.plugin.js"
  }
}
